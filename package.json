{
  "name": "fft_nano",
  "version": "1.0.1",
  "description": "FarmFriendTerminal_nano (FFT_nano) â€” secure, containerized AI farm assistant.",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/0-CYBERDYNE-SYSTEMS-0/FFT_nano.git"
  },
  "bugs": {
    "url": "https://github.com/0-CYBERDYNE-SYSTEMS-0/FFT_nano/issues"
  },
  "homepage": "https://github.com/0-CYBERDYNE-SYSTEMS-0/FFT_nano#readme",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "fft": "./bin/fft.js",
    "fft-nano": "./bin/fft.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "node --import tsx --test $(find tests container/agent-runner/src -type f -name '*.test.ts' | tr '\\n' ' ')",
    "fft": "node ./bin/fft.js",
    "tui": "node dist/tui/start.js",
    "tui:dev": "tsx src/tui/start.ts",
    "tui:client": "node dist/tui/client.js",
    "tui:client:dev": "tsx src/tui/client.ts",
    "onboard": "tsx src/onboard-cli.ts",
    "auth": "tsx src/whatsapp-auth.ts",
    "farm:doctor": "bash scripts/farm-doctor.sh",
    "validate:skills": "tsx scripts/validate-pi-skills.ts",
    "secret-scan": "bash scripts/secret-scan.sh",
    "pack-check": "bash scripts/check-pack-contents.sh",
    "release-check": "bash scripts/release-check.sh",
    "release:sha256": "bash scripts/release/generate-sha256s.sh",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@mariozechner/pi-tui": "^0.53.0",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
